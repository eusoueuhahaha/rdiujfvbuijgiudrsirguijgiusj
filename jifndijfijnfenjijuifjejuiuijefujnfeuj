local CheckMobile = function()
    if
        game:GetService("UserInputService").TouchEnabled
     then
        return true 
    end
  end 
  IsMobile = CheckMobile()
  local UserInputService = game:GetService("UserInputService")
  local RunService = game:GetService("RunService")
  local Players = game:GetService("Players")
  local LocalPlayer = Players.LocalPlayer
  
  local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
  local GuiService = game:GetService("GuiService")
  
  local screenResolution = GuiService:GetScreenResolution()
  local screenWidth = screenResolution.X
  local screenHeight = screenResolution.Y
  
  local windowWidth = screenWidth * 0.6
  local windowHeight = screenHeight * 0.6
  
  -- UDim2.fromOffset(580, 460),
  if not IsMobile then 
    windowWidth,windowHeight = 600,460  
  else
    local ClickButton = Instance.new("ScreenGui")
    local MainFrame = Instance.new("Frame")
    local ImageLabel = Instance.new("ImageLabel")
    local TextButton = Instance.new("TextButton") 
    local UICorner = Instance.new("UICorner") 
    local UICorner_2 = Instance.new("UICorner")
    if game.CoreGui:FindFirstChild("ClickButton") then 
        game.CoreGui:FindFirstChild("ClickButton"):Destroy()
    end
    ClickButton.Name = "ClickButton"
    ClickButton.Parent = game.CoreGui
    ClickButton.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ClickButton
    MainFrame.Active = true
    MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    MainFrame.BackgroundColor3 = Color3.new(1, 1, 1)
    MainFrame.BorderColor3 = Color3.new(0, 0, 0)
    MainFrame.BorderSizePixel = 0
    MainFrame.Transparency = 1
    MainFrame.Position = UDim2.new(0.187441245, 0, 0.476932675, 0)
    MainFrame.Size = UDim2.new(0, 45, 0, 45)
    
    UICorner.CornerRadius = UDim.new(0, 100)
    UICorner.Parent = MainFrame
    
    UICorner_2.CornerRadius = UDim.new(0, 100)
    UICorner_2.Parent = ImageLabel
    
    ImageLabel.Parent = MainFrame
    ImageLabel.AnchorPoint = Vector2.new(0.5, 0.5)
    ImageLabel.BackgroundColor3 = Color3.new(0, 0, 0)
    ImageLabel.BorderColor3 = Color3.new(0, 0, 0)
    ImageLabel.BorderSizePixel = 0
    ImageLabel.Position = UDim2.new(0.48888889, 0, 0.48888889, 0)
    ImageLabel.Size = UDim2.new(0, 45, 0, 45)
    ImageLabel.Image = "rbxassetid://15892713192"
    
    TextButton.Parent = MainFrame
    TextButton.BackgroundColor3 = Color3.new(1, 1, 1)
    TextButton.BackgroundTransparency = 1
    TextButton.BorderColor3 = Color3.new(0, 0, 0)
    TextButton.BorderSizePixel = 0
    TextButton.Position = UDim2.new(3.3908421e-07, 0, 0, 0)
    TextButton.Size = UDim2.new(0, 45, 0, 45)
    TextButton.AutoButtonColor = false
    TextButton.Font = Enum.Font.SourceSans
    TextButton.Text = ""
    TextButton.TextColor3 = Color3.new(255, 255, 255)
    TextButton.TextSize = 15
    TextButton.MouseButton1Click:Connect(function()
        game:GetService("VirtualInputManager"):SendKeyEvent(true,"LeftControl",false,game)
        game:GetService("VirtualInputManager"):SendKeyEvent(false,"LeftControl",false,game)
    end)
  end  
  
  local Window = Fluent:CreateWindow({
    Title = "TsuoHub",
    SubTitle = "developed by mani-",
    TabWidth = 160,
    Size = UDim2.new(0, windowWidth, 0, windowHeight),
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
  })
  
  -- Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
  local Tabs = {
    Credits = Window:AddTab({
        Title = "Discord",
        Icon = "gem"
    }),
    Main = Window:AddTab({
        Title = "Main",
        Icon = "banknote"
    }),
    ItemQuest = Window:AddTab({
        Title = "Item & Quest",
        Icon = "gamepad"
    }),
    StatsCombat = Window:AddTab({
        Title = "Stats & Combat",
        Icon = "coins"
    }),
    Shop = Window:AddTab({
        Title = "Shop",
        Icon = "egg"
    }),
    Teleports = Window:AddTab({
        Title = "Teleports",
        Icon = "aperture"
    }),
    Dungeon = Window:AddTab({
        Title = "Dungeon",
        Icon = "settings"
    }),
    Misc = Window:AddTab({
        Title = "Misc",
        Icon = "settings"
    }),
}
-- credits

Tabs.Credits:AddButton({
    Title = "Tsuo Discord Server (Click 1x)",
    Description = "Copy Link Discord Server",
    Callback = function()
       setclipboard("https://discord.com/invite/tsuoscripts")
    end
})

-- varivaies 

if game.PlaceId == 4520749081 then
	OldWorld = true
elseif game.PlaceId == 6381829480 then
	NewWorld = true
elseif game.PlaceId == 6596144663 then
	AwakenDun = true
end

if not _G.Method then
	_G.Method = "Behind"
end
if not _G.Distance then
	_G.Distance = 8
end
if not _G.BusoHaki or not _G.KenHaki then
	_G.BusoHaki = true
	_G.KenHaki = true
end
if not _G.SkillZ then
	_G.SkillZ = true
end
if not _G.SkillX then
	_G.SkillX = true
end
if not _G.SkillC then
	_G.SkillC = true
end
if not _G.SkillV then
	_G.SkillV = true
end
if not _G.SkillE then
	_G.SkillE = true
end
if not _G.SkillB then
	_G.SkillB = true
end
if not _G.AntiSit then
	_G.AntiSit = true
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoFarmMonster then
                if game:GetService("Workspace").Monster.Mon:FindFirstChild(_G.SelectMonster) then
                    for i,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                        if v.Name == tostring(_G.SelectMonster) then
                            if v.Humanoid.Health > 0 then
                                if v:FindFirstChild("HumanoidRootPart") then
                                    repeat task.wait()
                                        EquipWeapon(_G.Weapon)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                        UseSkillFarm = true
                                        getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                        Attack()
                                    until v.Humanoid.Health <= 0 or not _G.AutoFarmMonster
                                    UseSkillFarm = false
                                end
                            end
                        end
                    end
                elseif game:GetService("Workspace").Monster.Boss:FindFirstChild(_G.SelectMonster) then
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == tostring(_G.SelectMonster) then
                            if v.Humanoid.Health > 0 then
                                if v:FindFirstChild("HumanoidRootPart") then
                                    repeat task.wait()
                                        EquipWeapon(_G.Weapon)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                        UseSkillFarm = true
                                        getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                        Attack()
                                    until v.Humanoid.Health <= 0 or not _G.AutoFarmMonster
                                    UseSkillFarm = false
                                end
                            end
                        end
                    end
                end
            end
        end) 
    end
end)

function BodyClip()
    if not game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("Normal Hub") then
        local Noclip = Instance.new("BodyVelocity")
        Noclip.Name = "Normal Hub"
        Noclip.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
        Noclip.MaxForce = Vector3.new(100000,100000,100000)
        Noclip.Velocity = Vector3.new(0,0,0)
    end
end

function ClickGui_True()
    game:GetService'VirtualUser':Button1Down(Vector2.new(0.9,0.9))
    game:GetService'VirtualUser':Button1Up(Vector2.new(0.9,0.9))
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoNewWorld and OldWorld then
                BodyClip()
                if game.Players.LocalPlayer.PlayerStats.SecondSeaProgression.Value == "Yes" then
                    _G.AutoFarmLevel = false
                    TP(CFrame.new(-2406.879638671875, 16.195701599121094, -4362.17529296875))
                    wait(.5)
                    ClickGui_True()
                    wait(.5)
                    for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                        if v.Name == "Dialogue" then
                            v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                            v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                            v.Accept.ImageTransparency = 1
                            ClickGui_True()
                        end
                    end
                elseif game.Players.LocalPlayer.PlayerStats.SecondSeaProgression.Value == "No" then
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value >= 2250 then
                        _G.AutoFarmLevel = false
                        if game:GetService("Players")["LocalPlayer"].PlayerGui.Stats.Button["Inventory_Frame"].ScrollingFrameMaterial:FindFirstChild("Map") then
                            TP(CFrame.new(2608.053466796875, 211.32774353027344, -1808.430908203125))
                            ClickGui_True()
                            wait(.5)
                            for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                                if v.Name == "Dialogue" then
                                    v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                                    v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                                    v.Accept.ImageTransparency = 1
                                    ClickGui_True()
                                end
                            end
                        else
                            if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
                                TP(CFrame.new(2608.053466796875, 211.32774353027344, -1808.430908203125))
                                ClickGui_True()
                                wait(.5)
                                for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                                    if v.Name == "Dialogue" then
                                        v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                                        v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                                        v.Accept.ImageTransparency = 1
                                        ClickGui_True()
                                    end
                                end
                            else
                                if game:GetService("Workspace").Monster.Boss:FindFirstChild("Seasoned Fishman [Lv. 2200]") then
                                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                                        if v.Name == "Seasoned Fishman [Lv. 2200]" and v.Humanoid.Health > 0 then
                                            repeat task.wait()
                                                EquipWeapon(_G.Weapon)
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                                Attack()
                                            until v.Humanoid.Health <= 0 or game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false or not _G.AutoFarmLevel
                                        end
                                    end
                                else
                                    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Seasoned Fishman [Lv. 2200]") then
                                        TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("Seasoned Fishman [Lv. 2200]"):GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance) )
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end)

function GetNameMt()
    if _G.IsMt == "Leather [w1,w2]" then
        if OldWorld then
            return "Clown Pirate [Lv. 10]"
        else
            return "Beast Pirate [Lv. 2250]"
        end
    elseif _G.IsMt == "Gunpowder [w1,w2]" then
        if OldWorld then
            return "Dory [Lv. 350]"
        else
            return "Fiore Gladiator [Lv. 3600]"
        end
    elseif _G.IsMt == "Rusted Scrap [w1,w2]" then
        if OldWorld then
            return "New World Pirate [Lv. 1700]"
        else
            return "Beast Swordman [Lv. 2300]"
        end
    elseif _G.IsMt == "Undead's Ooze [w1,w2]" then
        if OldWorld then
            return "Zombie [Lv. 1500]"
        else
            return 'Dark Beard Servant [Lv. 3400]'
        end
    elseif _G.IsMt == "Vampire's [w1]" then
        return "Shadow Master [Lv. 1600]"
    elseif _G.IsMt == "Fresh Fish [w1]" then
        return "Fighter Fishman [Lv. 180]"
    elseif _G.IsMt == "Twilight's Orb [w1]" then
        return "Shadow Master [Lv. 1600]"
    elseif _G.IsMt == "Angelic Feather [w1]" then
        return "Ball Man [Lv. 850]"
    elseif _G.IsMt == "Carrot [w2]" then
        return "Beast Pirate [Lv. 2250]"
    elseif _G.IsMt == "Iron Ingot [w2]" then
        return "Elite Beast Pirate [Lv. 2700]"
    elseif _G.IsMt == "Bread Crumbs [w2]" then
        return "Chess Soldier [Lv. 3200]"
    elseif _G.IsMt == "Pile of Bones [w2]" then
        return "Elite Skeleton [Lv. 3100]"
    elseif _G.IsMt == "Samuri's Bage [w2]" then
        return "Kappa [Lv. 2950]" 
    elseif _G.IsMt == "Lost Ruby [w2]" then
        return "Anubis [Lv. 3150]"
    elseif _G.IsMt == "Essence of Fire [w2]" then
        return "Pharaoh [Lv. 3175]"
    elseif _G.IsMt == "Thief's rag [w2]" then
        return 'Desert Thief [Lv. 3125]'
    elseif _G.IsMt == "Dark Beard's Totem [w2]" then
        return "Dark Beard Servant [Lv. 3400]"
    elseif _G.IsMt == "Lucidus's Totem [w2]" then
        return "Vice Admiral [Lv. 3500]"
    end
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoFarmMaterial then
                _G.IsMOB = GetNameMt()
                if game:GetService("Workspace").Monster.Mon:FindFirstChild(_G.IsMOB) then
                    for i,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                        if v.Name == tostring(_G.IsMOB) then
                            if v.Humanoid.Health > 0 then
                                if v:FindFirstChild("HumanoidRootPart") then
                                    repeat task.wait()
                                        EquipWeapon(_G.Weapon)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                        UseSkillFarm = true
                                        getgenv().PosMonSkill = v:GetModelCFrame()
                                        Attack()
                                    until v.Humanoid.Health <= 0 or not _G.AutoFarmMaterial
                                end
                            end
                        end
                    end
                elseif game:GetService("Workspace").Monster.Boss:FindFirstChild(_G.IsMOB) then
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == tostring(_G.IsMOB) then
                            if v.Humanoid.Health > 0 then
                                if v:FindFirstChild("HumanoidRootPart") then
                                    repeat task.wait()
                                        EquipWeapon(_G.Weapon)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                        UseSkillFarm = true
                                        Attack()
                                        getgenv().PosMonSkill = v:GetModelCFrame()
                                    until v.Humanoid.Health <= 0 or not _G.AutoFarmMaterial
                                end
                            end
                        end
                    end
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.ReplicatedStorage.MOB:FindFirstChild(_G.IsMOB):GetModelCFrame() * CFrame.new(0,10,0)
                end
            end
        end)
    end
end)

function GetQuestDaily(Name)
    if not game:GetService("Workspace").AllNPC:FindFirstChild(tostring(Name)) then
        repeat task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP[tostring(Name)].CFrame
            game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC[tostring(Name)])
        until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild(tostring(Name)) or not _G.AutoFarmLevel
    else
        repeat task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC[tostring(Name)].CFrame
            game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC[tostring(Name)])
        until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild(tostring(Name)) or not _G.AutoFarmLevel
    end
    if	game:GetService("Workspace").AllNPC:FindFirstChild(tostring(Name)) then
        game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC[tostring(Name)])
        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild(tostring(Name)) then
            local ClickGui = game.Players.LocalPlayer.PlayerGui[tostring(Name)].Dialogue.Accept
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
        end
    end
end

function Quest_0(StaringQuest,NameMon,NamePosPuk,NamePosQuestDaily)
    if VisQuest() and string.find(DQ(),StaringQuest) then
        if game.Workspace.Monster.Mon:FindFirstChild(NameMon) or game.Workspace.Monster.Boss:FindFirstChild(NameMon) then
            Monhaha = quest[game.Players.LocalPlayer.CurrentQuest.Value].Mob
            for i,v in pairs(game:GetService("Workspace").Monster[GetM()]:GetChildren()) do
                if v.Name == tostring(Monhaha) then
                    if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                        repeat task.wait()
                            EquipWeapon(_G.Weapon)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                            UseSkillFarm = true
                            getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                            Attack()
                        until v.Humanoid.Health <= 0 or not VisQuest() or not _G.AutoBattlepass
                    else
                        TP(FindNPC(NamePosPuk).CFrame)
                    end
                end
            end
        else
            TP(FindNPC(NamePosPuk).CFrame)
        end
    else
        TP(FindNPC(NamePosQuestDaily).CFrame)
        wait(1)
        GetQuestDaily(NamePosQuestDaily)
    end
end

function DQ()
    return game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.QuestCount.Text
end

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoBattlepass then
                if _G.IsMode == "Quest : Lv. 0+" then
                    Quest_0("Soldier","Soldier [Lv. 1]","QuestLvl0","Daily QuestLvl0")
                elseif _G.IsMode == "Quest : Lv. 5+" then
                    if VisQuest() and string.find(DQ(),"'Old Man'") then
                        TP(FindNPC("Civilian Old").CFrame)
                    else
                        TP(FindNPC("Daily QuestLvl5").CFrame)
                        wait(.3)
                        ClickGui_True()
                        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Daily QuestLvl5") then
                            local ClickGui = game.Players.LocalPlayer.PlayerGui["Daily QuestLvl5"].Dialogue.Accept
                            game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                        end
                    end
                elseif _G.IsMode == "Quest : Lv. 500+" then
                    Quest_0("King Snow","King Snow [Lv. 450]","QuestLvl450","Daily QuestLvl500")
                elseif _G.IsMode == "Quest : Lv. 2000+" then
                    Quest_0("Soldier Fishman","Soldier Fishman [Lv. 2150]","QuestLvl2150","Daily QuestLvl2000")
                elseif _G.IsMode == "Quest : Lv. 3000+" then
                    Quest_0("Joey","Joey [Lv. 3000]","QuestLvl3000","Daily QuestLvl3000")
                elseif _G.IsMode == "Quest : Lv. 3500+" then
                    Quest_0("Hefty","Hefty [Lv. 3550]","QuestLvl3550","Daily QuestLvl3500")
                elseif _G.IsMode == "Quest Disobey : Lv. 3000+" then
                    if VisQuest() and string.find(DQ(),"Poster") then
                        TP(CFrame.new(-3884 , 61 , 205))
                        game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,game)
                        wait(.3)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false,"E",false,game)
                        wait(1)
                        TP(CFrame.new(-3826 , 57 , 93))
                        game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,game)
                        wait(.3)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false,"E",false,game)
                        wait(1)
                        TP(CFrame.new(-3716 , 61 , 210))
                        game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,game)
                        wait(.3)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false,"E",false,game)
                        wait(1)
                    else
                        TP(FindNPC("Daily Quest Disobey").CFrame)
                        wait(.3)
                        ClickGui_True()
                        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Daily Quest Disobey") then
                            local ClickGui = game.Players.LocalPlayer.PlayerGui["Daily Quest Disobey"].Dialogue.Accept
                            game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                        end
                    end
                elseif _G.IsMode == "Quest DeadAbove : Lv. 3000+" then
                    if VisQuest() and string.find(DQ(),"Floating Feather") then
                        TP(game.Workspace.SpawnItem['Floating Feather'].CFrame)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.SpawnItem['Floating Feather'].CFrame
                        fireproximityprompt(game.Workspace.SpawnItem['Floating Feather'].ProximityPrompt,math.huge)
                    else
                        TP(FindNPC("Daily Quest DeadAbove").CFrame)
                        wait(.3)
                        ClickGui_True()
                        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Daily Quest DeadAbove") then
                            local ClickGui = game.Players.LocalPlayer.PlayerGui["Daily Quest DeadAbove"].Dialogue.Accept
                            game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                        end
                    end
                elseif _G.IsMode == "Quest : Lv. 3800+" then
                    if VisQuest() and string.find(DQ(),"Book") then
                        TP(game.Workspace.Island['H - Fiore']['Lost Book'].Book1.CFrame)
                        game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,game)
                        wait(.3)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false,"E",false,game)
                    else
                        TP(FindNPC("Daily QuestLvl3800").CFrame)
                        wait(.3)
                        ClickGui_True()
                        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Daily QuestLvl3800") then
                            local ClickGui = game.Players.LocalPlayer.PlayerGui["Daily QuestLvl3800"].Dialogue.Accept
                            game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                        end
                    end
                elseif _G.IsMode == "Quest Boxes : Lv. 3000+" then
                    if VisQuest() and string.find(DQ(),"Boxes") then
                        _G.CheckBox = tostring(game.Players.LocalPlayer.Name).."QuestBox"
                        if _G.CheckBox ~= nil and game.Workspace.PlayerCharacters[game.Players.LocalPlayer.Name]:FindFirstChild(_G.CheckBox) then
                            TP(FindNPC("Civilian Port").CFrame)
                            ClickGui_True()
                            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Civilian Port") then
                                local ClickGui = game.Players.LocalPlayer.PlayerGui["Civilian Port"].Dialogue.Accept
                                game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                            end
                        elseif not game.Workspace.PlayerCharacters[game.Players.LocalPlayer.Name]:FindFirstChild(_G.CheckBox) then
