                game:GetService("Workspace"):FindFirstChild("Rig").Humanoid:BreakJoints()
                game:GetService("Workspace"):FindFirstChild("Rig"):BreakJoints()
            elseif _G.AutoAwakenOne and AwakenDun and syn then
                if isnetworkowner(game:GetService("Workspace"):FindFirstChild("Rig")) then
                    game:GetService("Workspace"):FindFirstChild("Rig").Humanoid:BreakJoints()
                    game:GetService("Workspace"):FindFirstChild("Rig"):BreakJoints()
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoAwakenOne then
                if AwakenDun then
                    local HealthMin = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * _G.SafeAt/100
                    if not game:GetService("Workspace").Effects:FindFirstChild("NewTsunami") then
                        repeat task.wait()
                            _G.HasMon = true
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame * CFrame.new(0,0,5)
                            PosMon = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame
                            getgenv().PosMonSkill = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame
                            UseSkillFarm = true
                            Attack()
                        until not _G.AutoAwakenOne or game.Players.LocalPlayer.Character.Humanoid.Health < HealthMin or game:GetService("Workspace").Effects:FindFirstChild("NewTsunami")
                    elseif game:GetService("Workspace").Effects:FindFirstChild("NewTsunami") then
                        repeat task.wait()
                            _G.HasMon = false
                            UseSkillFarm = false
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-72.14241027832031, 796.9161987304688, -5.780326843261719)
                        until not _G.AutoAwakenOne or not game:GetService("Workspace").Effects:FindFirstChild("NewTsunami")
                    end
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Awake Master").CFrame
                    wait(1)
                    ClickGui_True()
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Awake Master") then
                        local ClickGui = game.Players.LocalPlayer.PlayerGui["Awake Master"].Dialogue.Accept
                        game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoAwaken then
                if AwakenDun and _G.AutoSafeat then
                    local HealthMin = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * _G.SafeAt/100
                    if game.Players.LocalPlayer.Character.Humanoid.Health >= HealthMin and not game:GetService("Workspace").Effects:FindFirstChild("NewTsunami") then
                        repeat task.wait()
                            _G.HasMon = true
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame * CFrame.new(0,0,5)
                            PosMon = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame
                            getgenv().PosMonSkill = game:GetService("Workspace").Rig.HumanoidRootPart.CFrame
                            UseSkillFarm = true
                            Attack()
                        until not _G.AutoAwaken or game.Players.LocalPlayer.Character.Humanoid.Health < HealthMin or game:GetService("Workspace").Effects:FindFirstChild("NewTsunami")
                    elseif game.Players.LocalPlayer.Character.Humanoid.Health >= HealthMin and game:GetService("Workspace").Effects:FindFirstChild("NewTsunami") then
                        repeat task.wait()
                            _G.HasMon = false
                            UseSkillFarm = false
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-72.14241027832031, 796.9161987304688, -5.780326843261719)
                        until not _G.AutoAwaken or not game:GetService("Workspace").Effects:FindFirstChild("NewTsunami")
                    elseif game.Players.LocalPlayer.Character.Humanoid.Health < HealthMin then
                        repeat task.wait()
                            _G.HasMon = false
                            UseSkillFarm = false
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-72.14241027832031, 796.9161987304688, -5.780326843261719)
                            local args = {
                                [1] = "FS_Cyborg_E",
                                [2] = {
                                    ["Type"] = "Down",
                                    ["MouseHit"] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0)
                                }
                            }
                            game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                        until not _G.AutoAwaken or game.Players.LocalPlayer.Character.Humanoid.Health >= game.Players.LocalPlayer.Character.Humanoid.MaxHealth * _G.UntilAt/100
                    end
                elseif AwakenDun and not _G.AutoSafeat then
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "Auto Awaken",
                        Text = "Open Safe at ...",
                        Icon = "rbxassetid://",
                        Duration = 5,
                    })
                    wait(7)
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Awake Master").CFrame
                    wait(1)
                    ClickGui_True()
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Awake Master") then
                        local ClickGui = game.Players.LocalPlayer.PlayerGui["Awake Master"].Dialogue.Accept
                        game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        pcall(function()
            if _G.AutoDungeon then
                if not game:GetService("Workspace"):FindFirstChild("AA") then
                    local Part = Instance.new("Part")
                    Part.Name = "AA"
                    Part.Parent = game.Workspace
                    Part.Anchored = true
                    Part.Transparency = 1
                    Part.Size = Vector3.new(30,0.5,30)
                elseif game:GetService("Workspace"):FindFirstChild("AA") then
                    game.Workspace["AA"].CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y - 3.92,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z)
                end
            else
                if game:GetService("Workspace"):FindFirstChild("AA") then
                    game:GetService("Workspace"):FindFirstChild("AA"):Destroy()
                end
            end
        end)
    end)
end)
