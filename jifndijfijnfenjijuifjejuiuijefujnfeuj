function TP(p1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = p1
end

VirtualUser = game:GetService("VirtualUser")
quest = require(game:GetService("ReplicatedStorage").Modules.QuestManager)

function CheckLv()
    for i,v in pairs(quest) do
        for x,z in pairs(v) do
            if x == "Level" then
                if game.Players.LocalPlayer.PlayerStats.lvl.Value >= z then
                    if not questlv then
                        questlv = 1
                    end
                    if z >= questlv then
                        questlv = z
                    end
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value == 1 or game.Players.LocalPlayer.PlayerStats.lvl.Value <= 15 then
                        questlv = 0
                    end
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value >= 3325 and game.Players.LocalPlayer.PlayerStats.lvl.Value < 3400 then
                        questlv = 3325
                    end
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value >= 3475 and game.Players.LocalPlayer.PlayerStats.lvl.Value < 3500 then
                        questlv = 3450
                    end
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value >= 3575 and game.Players.LocalPlayer.PlayerStats.lvl.Value <= 3600 then
                        questlv = 3550
                    end
                    if game.Players.LocalPlayer.PlayerStats.lvl.Value == 3800 then
                        questlv = 3775
                    end
                    
                    if game.PlaceId == 4520749081 then
                        if questlv >= 2200 then
                            questlv = 2200
                        end
                    end
                    if questlv == 750 then
                        questlv = 725
                    end
                end
            end
        end
    end
end


function Attack()
    coroutine.wrap(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") then
                if v.ToolTip == "Sword" then
                    TypeWeapon = "SW"
                elseif v.ToolTip == "Combat" then
                    TypeWeapon = "FS"
                end
            
                if TypeWeapon then
                    local args = {
                        [1] = TypeWeapon.."_"..v.Name.."_M1"
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                end
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Tool") then
                if v.ToolTip == "Sword" then
                    TypeWeapon = "SW"
                elseif v.ToolTip == "Combat" then
                    TypeWeapon = "FS"
                end
            
                if TypeWeapon then
                    local args = {
                        [1] = TypeWeapon.."_"..v.Name.."_M1"
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                end
            end
        end
    end)()
end

function VisQuest()
    return game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible
end


function EquipWeapon(ToolSe)
    local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
    game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
end

if not TRIGON_LOADED then
    local mt = getrawmetatable(game)
    local old = mt.__namecall
    setreadonly(mt,false)
    mt.__namecall = function(...)
        local args = {...}
        local method = getnamecallmethod()
        if method == "InvokeServer" then
            if tostring(args[1]) == "SkillAction" then
                if getgenv().PosMonSkill then
                    if not args[3] then
                        return old(...)
                    end
                    if not (_G.SkillZ or _G.SkillX or _G.SkillC or _G.SkillV or _G.SkillB or _G.SkillE) then
                        return old(...)
                    end
                    if args[3].Type == "Up" or args[3].Type == "Down" then
                        args[3].MouseHit = getgenv().PosMonSkill
                        return old(unpack(args))
                    end
                end
            end
        end
        return old(...)
    end
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if UseSkillFarm then
                if _G.SkillZ then
                    print('z')
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"Z",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"Z",false,game)
                end
                if _G.SkillX then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"X",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"X",false,game)
                end
                if _G.SkillC then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"C",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"C",false,game)
                end
                if _G.SkillV then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"V",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"V",false,game)
                end
                if _G.SkillB then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"B",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"B",false,game)
                end
                if _G.SkillE then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,game)
                    wait(.3)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"E",false,game)
                end
            end
        end)
    end
end)

local i = game.Players.LocalPlayer
function QuestAccept()
if getrenv()._G.RenderDist1 ~= 100000 then 
    getrenv()._G.RenderDist1 = 100000
end
for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do 
    if v:FindFirstChild("Dialogue") then 
        local TextQ = v.Dialogue
        if not TextQ:FindFirstChild("Accept") then 
            game.Players.LocalPlayer.Character:BreakJoints()
        end
        task.spawn(function()
            pcall(function()
                TextQ.Accept.BackgroundTransparency = 1
                TextQ.Accept.ImageTransparency = 1
                TextQ.Accept:ClearAllChildren()
            end)
        end)
        task.spawn(function()
            pcall(function()
                TextQ.Accept.Position = UDim2.new(0,-700,0,-700)
            end)
        end)
        task.spawn(function()
            pcall(function()
                TextQ.Accept.Size = UDim2.new(100,100,100,100)
            end)
        end)
    end
end
wait()
ClickGui_True()
end

function GetM()
    if VisQuest() then
        for i,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
            if v.Name == quest[game.Players.LocalPlayer.CurrentQuest.Value].Mob then
                return tostring(v.Parent)
            end
        end
        for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
            if v.Name == quest[game.Players.LocalPlayer.CurrentQuest.Value].Mob then
                return tostring(v.Parent)
            end
        end
    end
end

function GetQuest()
    if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..tostring(questlv)) then
        repeat task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..tostring(questlv)].CFrame
            game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC["QuestLvl"..tostring(questlv)])
        until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(questlv)) or not _G.AutoFarmLevel
    else
        repeat task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..tostring(questlv)].CFrame
            game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC["QuestLvl"..tostring(questlv)])
        until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(questlv)) or not _G.AutoFarmLevel
    end
    QuestAccept()
    if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..tostring(questlv)) then
        game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC["QuestLvl"..tostring(questlv)])
        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(questlv)) then
            QuestAccept()
            --local ClickGui = game.Players.LocalPlayer.PlayerGui["QuestLvl"..tostring(questlv)].Dialogue.Accept
            --game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
        end
    end
    
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoFarmLevel then
                if VisQuest() then
                    if OldWorld then
                        if game:GetService("Players")["LocalPlayer"].PlayerGui.Quest.QuestBoard.QuestCount == "0/1 Golden Key" then
                            game:GetService("Players")["LocalPlayer"].PlayerGui.Quest.QuestBoard.Visible = false
                        end
                    elseif NewWorld then
                        if game.Players.LocalPlayer.PlayerStats.lvl.Value < 3300 and Monhaha == "Dough Master [Lv. 3275]" and not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dough Master [Lv. 3275]") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(30272.3203125, 65.4236068725586, 93207.0234375)
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while wait() do
        pcall(function()
            if _G.AutoFarmLevel then
                CheckLv()
                if not VisQuest() then
                    UseSkillFarm = false
                    GetQuest()
                else
                    Monhaha = quest[game.Players.LocalPlayer.CurrentQuest.Value].Mob
                    for i,v in pairs(game:GetService("Workspace").Monster[GetM()]:GetChildren()) do
                        if v.Name == tostring(Monhaha) then
                            if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                repeat task.wait()
                                    EquipWeapon(_G.Weapon)
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                    UseSkillFarm = true
                                    getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                    Attack()
                                until v.Humanoid.Health <= 0 or not VisQuest() or not _G.AutoFarmLevel
                            else
                                UseSkillFarm = false
                                if v.Name == "Dough Master [Lv. 3275]" then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(30272.3203125, 65.4236068725586, 93207.0234375)
                                end
                                if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..tostring(questlv)) then
                                    repeat task.wait()
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..tostring(questlv)].CFrame
                                        game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC["QuestLvl"..tostring(questlv)])
                                        QuestAccept()
                                    until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(questlv)) or not _G.AutoFarmLevel
                                else
                                    repeat task.wait()
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..tostring(questlv)].CFrame
                                        game:GetService("ReplicatedStorage").Remotes.Functions.CheckQuest:InvokeServer(workspace.AllNPC["QuestLvl"..tostring(questlv)])
                                        QuestAccept()
                                    until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(questlv)) or not _G.AutoFarmLevel
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end)

local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
    vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
end)


function FindChest()
    for i,v in pairs(game:GetService("Workspace").EventSpawns:GetChildren()) do
        for i2,v2 in pairs(v:GetChildren()) do
            if v2.Name == "Chest" then
                return v2.Parent
            end
        end
    end
    return false
end
function CheckChest()
    for i,v in pairs(game:GetService("Workspace").EventSpawns:GetChildren()) do
        for i2,v2 in pairs(v:GetChildren()) do
            if v2.Name == "Chest" then
                return "Chest Spawn"
            end
        end
    end
    return "Chest Not Spawn"
end
task.spawn(function()
    while wait() do
        pcall(function()
            if _G.AutoMonsterEvent then
                if FindChest() ~= false then
                    repeat wait()
                        TP(FindChest().CFrame)
                        wait(10)
                        FindChest():FindFirstChild("Chest"):Destroy()
                    until FindChest() == false or _G.AutoMonsterEvent == false
                elseif FindChest() == false then
                    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Boss") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Minion") then
                        for i,v in pairs(game:GetService("ReplicatedStorage").MOB:GetChildren()) do
                            if v.Name == "Boss" or v.Name == "Minion" then
                                if v.Humanoid.Health > 0 then
                                    if v:FindFirstChild("HumanoidRootPart") then
                                        repeat task.wait()
                                            EquipWeapon(tostring(game:GetService("Players").LocalPlayer.Backpack:FindFirstChildOfClass("Tool")))
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:GetModelCFrame() * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,12)
                                            UseSkillFarm = true
                                            Attack()
                                            getgenv().PosMonSkill = v:GetModelCFrame()
                                        until not _G.AutoMonsterEvent or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                        UseSkillFarm = false
                                    end
                                end
                            end
                        end
                    elseif game:GetService("Workspace").Monster.Mon:FindFirstChild("Minion") or game:GetService("Workspace").Monster.Mon:FindFirstChild("Boss") then
                        for i,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                            if v.Name == "Boss" or v.Name == "Minion" then
                                if v.Humanoid.Health > 0 then
                                    if v:FindFirstChild("HumanoidRootPart") then
                                        repeat task.wait()
                                            EquipWeapon(tostring(game:GetService("Players").LocalPlayer.Backpack:FindFirstChildOfClass("Tool")))
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:GetModelCFrame() * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,12)
                                            UseSkillFarm = true
                                            Attack()
                                            getgenv().PosMonSkill = v:GetModelCFrame()
                                        until not _G.AutoMonsterEvent or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                        UseSkillFarm = false
                                    end
                                end
                            end
                        end
                    else
                        if _G.AutoMonsterEventHop then
                            Hop()
                        else
                            repeat wait()
                                Tebal = {}
                                for i,v in pairs(game:GetService("Workspace").EventSpawns:GetChildren()) do
                                    if v.Name == "Spawn" then
                                        table.insert(Tebal,v.CFrame)
                                    end
                                end
                                for i = 1,15 do
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Tebal[i] * CFrame.new(0,50,0)
                                    wait(0.5)
                                end
                            until FindChest() ~= false or game:GetService("Workspace").Monster.Mon:FindFirstChild("Minion") or game:GetService("Workspace").Monster.Mon:FindFirstChild("Boss") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Boss") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Minion") or not _G.AutoMonsterEvent
                        end
                    end
                end
            end
        end)
    end
end)


local AutoFarmLevel = Tabs.Main:AddToggle("AutoFarmLevel", {Title = "Auto-Farm-Level", Default = false })

AutoFarmLevel:OnChanged(function(vu)
    _G.AutoFarm = vu
          Magnet = vu
      if _G.AutoFarm and SelectToolWeapon == "" then
          ui:Notification("AutoFarm","SelectWeapon First ",2)
      else
          Auto_Farm = vu
          SelectMonster = ""
          if vu == false then
              wait(1)
              TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
          end
      end
         function UseCode(Text)
          game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer(Text)
      end
      UseCode("UPD16")
      UseCode("2BILLION")
      UseCode("UPD15")
      UseCode("FUDD10")
      UseCode("BIGNEWS")
      UseCode("THEGREATACE")
      UseCode("SUB2GAMERROBOT_EXP1")
      UseCode("StrawHatMaine")
      UseCode("Sub2OfficialNoobie")
      UseCode("SUB2NOOBMASTER123")
      UseCode("Sub2Daigrock")
      UseCode("Axiore")
      UseCode("TantaiGaming")
      UseCode("STRAWHATMAINE") 
    end)

local AutoNewWorld = Tabs.Main:AddToggle("AutoNewWorld", {Title = "Auto-New-World", Default = false })

AutoNewWorld:OnChanged(function(value)
    _G.AutoNewWorld = value
	
    if not value then
        UseSkillFarm = false
    end
end)

local AutoEventChest = Tabs.Main:AddToggle("AutoEventChest", {Title = "Auto-Event-Chest", Default = false })

AutoEventChest:OnChanged(function(value)
    _G.AutoMonsterEvent = value
	
    if not value then
        UseSkillFarm = false
    end
end)

