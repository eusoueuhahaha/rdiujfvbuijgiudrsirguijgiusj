local CD = 0
local function My(a)
    if a then
        if tick() >= CD then
            CD = tick() + 1
            game.Players.LocalPlayer.Character.Services.Client.KenEvent:InvokeServer(true)
        end
    end
end

task.spawn(function()
    while task.wait() do
        if _G.KenHaki then
            pcall(function()
                My(_G.KenHaki)
            end)
        end
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoKaido then
                BodyClip()
                if _G.AutoKaidoHop then
                    if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") and not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
                        Hop()
                    end
                end
                if game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == "Dragon [Lv. 5000]" and v.Humanoid.Health > 0 then
                            repeat task.wait()
                                UseSkillFarm = true
                                getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                EquipWeapon(_G.Weapon)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame  * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                Attack()
                            until not _G.AutoKaido or v.Humanoid.Health <= 0
                        end
                    end
                elseif game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
                    TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]"):GetModelCFrame() * CFrame.new(0,50,0))
                else
                    if _G.SpawnBoss then
                        if game:GetService("Players")["LocalPlayer"].PlayerGui.Stats.Button["Inventory_Frame"].ScrollingFrameMaterial:FindFirstChild("Dragon's Orb") then
                            TP(CFrame.new(-5997.35546875, 462.1614685058594, 7297.046875)*CFrame.new(0,0,-2))
                            ClickGui_True()
                            wait(.6)
                            if game:GetService("Players")["LocalPlayer"].PlayerGui:FindFirstChild("SummonDragon") then
                                local ClickGui = game.Players.LocalPlayer.PlayerGui["SummonDragon"].Dialogue.Accept
                                game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                            end
                        else
                            if game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                                for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                                    if v.Name == "Elite Skeleton [Lv. 3100]" and v.Humanoid.Health > 0 then
                                        repeat task.wait()
                                            UseSkillFarm = true
                                            getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                            EquipWeapon(_G.Weapon)
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                            Attack()
                                        until not _G.AutoKaido or v.Humanoid.Health <= 0 or game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]")
                                    end
                                end
                            else
                                if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                                    TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("Elite Skeleton [Lv. 3100]"):GetModelCFrame() * CFrame.new(0,50,0))
                                else
                                    TP(CFrame.new(-5997.35546875, 462.1614685058594, 7297.046875))
                                end
                            end
                        end
                    else
                        if game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                            for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                                if v.Name == "Elite Skeleton [Lv. 3100]" and v.Humanoid.Health > 0 then
                                    repeat task.wait()
                                        UseSkillFarm = true
                                        getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                        EquipWeapon(_G.Weapon)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                        Attack()
                                    until not _G.AutoKaido or v.Humanoid.Health <= 0 or game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]")
                                end
                            end
                        else
                            if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                                TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("Elite Skeleton [Lv. 3100]"):GetModelCFrame() * CFrame.new(0,50,0))
                            else
                                TP(CFrame.new(-5997.35546875, 462.1614685058594, 7297.046875))
                            end
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.MsMother then
                BodyClip()
                if game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
