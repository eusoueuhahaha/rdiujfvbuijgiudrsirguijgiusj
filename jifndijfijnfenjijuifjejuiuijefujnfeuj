function ESK()
    for i,v in pairs(game:GetService("Workspace").Island:GetChildren()) do
        pcall(function()
            if string.find(v.Name,"Legacy Island") then
                if _G.ESPSK then
                    if string.find(v.Name, "Fruit") then
                        if not v.ChestSpawner:FindFirstChild("LegacyIslandESP") then
                            local Bb = Instance.new("BillboardGui", v.ChestSpawner)
                            Bb.Name = "LegacyIslandESP"
                            Bb.ExtentsOffset = Vector3.new(0, 1, 0)
                            Bb.Size = UDim2.new(1, 200, 1, 30)
                            Bb.Adornee = v.ChestSpawner
                            Bb.AlwaysOnTop = true
                            local Textlb = Instance.new("TextLabel", Bb)
                            Textlb.Font = "GothamBold"
                            Textlb.FontSize = "Size14"
                            Textlb.Size = UDim2.new(1,0,1,0)
                            Textlb.BackgroundTransparency = 1
                            Textlb.TextStrokeTransparency = 0.5
                            Textlb.Text = v.Name.."\n"..math.round((v.ChestSpawner.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude/3).." m."
                            Textlb.TextColor3 = Color3.new(255, 255, 255)
                        else
                            v.ChestSpawner["LegacyIslandESP"].TextLabel.Text = v.Name.."\n"..math.round((v.ChestSpawner.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude/3).." m."
                        end
                    end
                else
                    if v.ChestSpawner:FindFirstChild("LegacyIslandESP") then
                        v.ChestSpawner:FindFirstChild("LegacyIslandESP"):Destroy()
                    end
                end
            end
        end)
    end
end

