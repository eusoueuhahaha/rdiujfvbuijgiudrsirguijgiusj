function Hop()
    local Dy = game.PlaceId
    local Iy = {}
    local wy = ""
    local jy = os.date("!*t").hour
    local Py = false
    function TPReturner()
        local a
        if wy == "" then
            a =
                game.HttpService:JSONDecode(
                game:HttpGet("https://games.roblox.com/v1/games/" .. Dy .. "/servers/Public?sortOrder=Desc&limit=100")
            )
        else
            a =
                game.HttpService:JSONDecode(
                game:HttpGet(
                    "https://games.roblox.com/v1/games/" .. Dy .. "/servers/Public?sortOrder=Desc&limit=100&cursor=" .. wy
                )
            )
        end
        local Xs = ""
        if a.nextPageCursor and a.nextPageCursor ~= "null" and a.nextPageCursor ~= nil then
            wy = a.nextPageCursor
        end
        local Os = 0
        for cW, wW in pairs(a.data) do
            local TW = true
            Xs = tostring(wW.id)
            if tonumber(wW.maxPlayers) > tonumber(wW.playing) then
                for uO, ZO in pairs(Iy) do
                    if Os ~= 0 then
                        if Xs == tostring(ZO) then
                            TW = false
                        end
                    else
                        if tonumber(jy) ~= tonumber(ZO) then
                            local om =
                                pcall(
                                function()
                                    Iy = {}
                                    table.insert(Iy, jy)
                                end
                            )
                        end
                    end
                    Os = Os + 1
                end
                if TW == true then
                    table.insert(Iy, Xs)
                    wait()
                    pcall(
                        function()
                            wait()
                            game:GetService("TeleportService"):TeleportToPlaceInstance(Dy, Xs, i)
                        end
                    )
                    wait(5)
                end
            end
        end
    end

    if getgenv().Teleport == nil then
        getgenv().Teleport = function()
            while task.wait() do
                pcall(function()
                    TPReturner()
                    if wy ~= "" then
                        TPReturner()
                    end
                end)
            end
        end
    end
    getgenv().Teleport()
end

function GetHydra()
    for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do
        if v.Name:find("Hydra") then
            return v
        end
    end
    return false
end

function GetFruit()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
        if v.Name:find("Fruit") then
            return v.Name
        end
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
        if v.Name:find("Fruit") then
            return v.Name
        end
    end
    return "Not Found"
end

function GetTire(Tire,Name)
    DFTire = require(game:GetService("ReplicatedStorage").Modules.DFTier)
    for i,v in pairs(DFTire[Tire]) do 
        if v == Name then
            return true
        end
    end
    return false
end

function getHydraIsland()
    for i,v in pairs(game:GetService("Workspace").Island:GetChildren()) do
        if v.Name:find("Sea King") then
            return true
        end
    end
    return false
end

function getHydraIName()
    for i,v in pairs(game:GetService("Workspace").Island:GetChildren()) do
        if v.Name:find("Sea King") then
            return tostring(v.Name:split("King ")[2])
        end
    end
end

task.spawn(function()
    while task.wait() do
        if _G.AutoHydraMonster then 
            pcall(function()
                BodyClip()
                if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then 
                    if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Health > 0 then 
                        pcall(function()
                            PosMonHydra = game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing"):GetModelCFrame()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").HB1.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                            EquipWeapon(_G.Weapon)
                            UseSkillFarm = true
                            IsFarmingHydra = true
                            getgenv().PosMonSkill = game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").HB1.CFrame
                            Attack()
                        end)
                    end
                else
                    if IsFarmingHydra then
                        IsFarmingHydra = false
                        UseSkillFarm = false
                        PosMonHydra = nil
                    end
                    if getHydraIsland() == true then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())].HydraStand.CFrame * CFrame.new(0,2,-2)
                    else
                        if _G.AutoHydraMonsterHop and NewWorld then
                            Hop()
                        end
                    end
                end
            end)
        end
    end
end)

task.spawn(function()
    while task.wait(2) do
        if _G.AutoHydraMonster then 
            if getHydraIsland() == true then
                pcall(function()
                    if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then 
                        if (game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Health <= 0 or not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing")) and not game:GetService("Workspace").SeaMonster:FindFirstChild("Hydra's Minion") then 
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())].HydraStand.CFrame * CFrame.new(0,2,-2)
                            delay(12, function()
                                pcall(function()
                                    if _G.AutoHydraMonsterHop then
                                        if (game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Health <= 0 or not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing")) and not game:GetService("Workspace").SeaMonster:FindFirstChild("Hydra's Minion") then
                                            if game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())] ~= nil then 
                                                game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())]:Remove()
                                            end
                                        end
                                    end
                                end)
                            end)
                        end
                    else 
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())].HydraStand.CFrame * CFrame.new(0,2,-2)
                    end
                end)
            end
        end
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.AutoGhostShip then
            pcall(function()
                BodyClip()
                if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
                    for i,v in pairs(game:GetService("Workspace"):FindFirstChild("GhostMonster"):GetChildren()) do
                        if v.Name == "Ghost Ship" and v.Humanoid.Health > 0 then
                            repeat task.wait()
                                UseSkillFarm = true
                                EquipWeapon(_G.Weapon)
                                getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                TP(v.HumanoidRootPart.CFrame * CFrame.new(0,10,0))
                                Attack()
                            until v.Humanoid.Health <= 0 or not _G.AutoGhostShip or not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship")
                        else
                            if game:GetService("Workspace"):FindFirstChild("Chest1") then
                                TP(game:GetService("Workspace").Chest1.RootPart.CFrame)
                                wait(.6)
                                TP(game:GetService("Workspace").Chest2.RootPart.CFrame)
                                wait(.6)
                                TP(game:GetService("Workspace").Chest3.RootPart.CFrame)
                                wait(.6)
                                TP(game:GetService("Workspace").Chest4.RootPart.CFrame)
                                wait(.6)
                                TP(game:GetService("Workspace").Chest5.RootPart.CFrame)
                                wait(.6)
                                TP(game:GetService("Workspace").Chest6.RootPart.CFrame)
                                wait(.6)
                            end
                        end
                    end
                else
                    if _G.AutoGhostShipHop and not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") and not game:GetService("Workspace"):FindFirstChild("Chest1") and not game:GetService("Workspace"):FindFirstChild("Chest2") and not game:GetService("Workspace"):FindFirstChild("Chest3") and not game:GetService("Workspace"):FindFirstChild("Chest4") and not game:GetService("Workspace"):FindFirstChild("Chest5") and not game:GetService("Workspace"):FindFirstChild("Chest6") then
                        wait(5)
                        repeat task.wait()
                        Hop()
                        until abvvv
                    end
                end
            end)
        end
    end
end)

