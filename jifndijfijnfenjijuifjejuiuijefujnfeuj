                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                Attack()
                                until not _G.TenguMask or v.Humanoid.Health <= 0 or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Ice Crystal")
                            end
                        end
                    else
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("QuestLvl3350").CFrame
                    end
                end
            end
        end)
    end
end)

