                                EquipWeapon(_G.Weapon)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                Attack()
                            until v.Humanoid.Health <= 0 or not _G.MsMother
                        end
                    end
                else
                    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
                        TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]"):GetModelCFrame() * CFrame.new(0,0,3))
                    else
