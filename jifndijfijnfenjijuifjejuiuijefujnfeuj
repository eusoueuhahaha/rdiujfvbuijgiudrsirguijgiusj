end)

task.spawn(function()
    while wait(0.6) do
        if methodnow == 1 then
            methodnow = 2 
            Method = CFrame.new(0,3,55)
        elseif methodnow == 2 then
            methodnow = 3
            Method = CFrame.new(55,3,0)
        elseif methodnow == 3 then
            methodnow = 4 
            Method = CFrame.new(0,3,-55)
        elseif methodnow == 4 then
            methodnow = 1 
            Method = CFrame.new(-55,3,0)
        else
            methodnow = 1 
            Method = CFrame.new(55,3,0)
        end
    end
end)

task.spawn(function()
    while wait(.5) do
        pcall(function()
            if DunWorld and _G.MethodDungeons == "FastMode" and _G.HasMon then
                for x,z in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if z.ClassName == "Tool" and z.Name ~= "LegacyPose" and z.Name ~= "Cyborg" then
                        EquipWeapon(z.Name)
                    end 
                end
            end
        end)
    end
end)

_G.Distance = 5
methodnow = 1 
task.spawn(function()
    while wait(.5) do
        if methodnow == 1 then  
            MethodDun = CFrame.new(70,_G.Distance,0)
            methodnow = 2 
        elseif methodnow == 2 then  
            MethodDun = CFrame.new(0,_G.Distance,70)
            methodnow = 3 
        elseif methodnow == 3 then  
            MethodDun = CFrame.new(-70,_G.Distance,0)
            methodnow = 4
        elseif methodnow == 4 then  
            MethodDun = CFrame.new(0,_G.Distance,-70)
            methodnow = 5
        elseif methodnow == 5 then  
            MethodDun = CFrame.new(0,_G.Distance,70)
            methodnow = 1
        else
            MethodDun = CFrame.new(0,_G.Distance,0)
            methodnow = 1
        end
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if DunWorld then 
                if _G.AutoDungeon then
                    game:GetService("Players").LocalPlayer.Character.Services.Client.KenEvent:InvokeServer(true)
                    game:GetService("Players").LocalPlayer.Character.SwimRemote:FireServer("Out")
                    _G.CooldownSkill = true
                    if _G.MethodDungeons == "FastMode" then
                        if _G.AutoSafeat then
                            local player = game.Players.LocalPlayer
                            local character = player.Character or player.CharacterAdded:Wait()
                            local humanoid = character:WaitForChild("Humanoid")
                            local HealthMin = humanoid.MaxHealth * _G.SafeAt/100
                            if game.Players.LocalPlayer.Character.Humanoid.Health >= HealthMin then
                                if game:GetService("Workspace").MOB:FindFirstChildOfClass("Model") and not game:GetService("Workspace").Effects:FindFirstChild("Part") then
                                    --game.Workspace.Gravity = 0
                                    for i,v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
                                        repeat task.wait()
                                            _G.HasMon = true
                                            TP(v:GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance))
                                            getgenv().PosMonSkill = v:GetModelCFrame()
                                            PosMon = v:GetModelCFrame()
                                            Attack()
                                            UseSkillDun = true
                                            UseSkillFarm = true
                                            spawn(function()
                                                game:GetService("VirtualInputManager"):SendKeyEvent(true,"Z",false,game)
                                                wait(.3)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(false,"Z",false,game)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(true,"X",false,game)
                                                wait(.3)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(false,"X",false,game)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(true,"C",false,game)
                                                wait(.3)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(false,"C",false,game)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(true,"V",false,game)
                                                wait(.3)
                                                game:GetService("VirtualInputManager"):SendKeyEvent(false,"V",false,game)
                                            end)
                                        until not _G.AutoDungeon or _G.HasMon == true
                                        UseSkillFarm = false
                                    end
                                else
                                    --game.Workspace.Gravity = 196.19999694824
                                    _G.HasMon = false
                                    UseSkillFarm = false
                                    UseSkillDun = false
                                    spawn(function()
                                        local args = {
                                            [1] = "FS_Cyborg_E",
                                            [2] = {
                                                ["Type"] = "Down",
                                                ["MouseHit"] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0)
                                            }
                                        }
                                        game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                                    end)
                                    wait(0.3)
                                    TP(game.Workspace.AllNPC["DUN"].CFrame * MethodDun)
                                end
                            elseif game.Players.LocalPlayer.Character.Humanoid.Health < HealthMin then
                                repeat task.wait()
                                    _G.HasMon = false
                                    UseSkillFarm = false
                                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                                    TP(game.Workspace.AllNPC["DUN"].CFrame * MethodDun)
                                    spawn(function()
                                        local args = {
                                            [1] = "FS_Cyborg_E",
                                            [2] = {
                                                ["Type"] = "Down",
                                                ["MouseHit"] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0)
                                            }
                                        }
                                        game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                                    end)
                                    TP(game.Workspace.AllNPC["DUN"].CFrame * MethodDun)
                                until not _G.AutoDungeon or not _G.AutoSafeat or game.Players.LocalPlayer.Character.Humanoid.Health >= humanoid.MaxHealth * _G.StopHeal/100
                            end
                        else
                            if game:GetService("Workspace").MOB:FindFirstChildOfClass("Model") and not game:GetService("Workspace").Effects:FindFirstChild("Part") then
                                --game.Workspace.Gravity = 0
                                for i,v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
                                    repeat task.wait()
                                        _G.HasMon = true
                                        TP(v:GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance))
                                        getgenv().PosMonSkill = v:GetModelCFrame()
                                        PosMon = v:GetModelCFrame()
                                        Attack()
                                        UseSkillDun = true
                                        UseSkillFarm = true
                                        spawn(function()
                                            game:GetService("VirtualInputManager"):SendKeyEvent(true,"Z",false,game)
                                            wait(.3)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(false,"Z",false,game)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(true,"X",false,game)
                                            wait(.3)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(false,"X",false,game)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(true,"C",false,game)
                                            wait(.3)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(false,"C",false,game)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(true,"V",false,game)
                                            wait(.3)
                                            game:GetService("VirtualInputManager"):SendKeyEvent(false,"V",false,game)
                                        end)
                                    until not _G.AutoDungeon or _G.HasMon == true
                                    UseSkillFarm = false
                                end
                            else
                                --game.Workspace.Gravity = 196.19999694824
                                _G.HasMon = false
                                UseSkillFarm = false
                                TP(game.Workspace.AllNPC["DUN"].CFrame * CFrame.new(0,5,0))
                                spawn(function()
                                    local args = {
                                        [1] = "FS_Cyborg_E",
                                        [2] = {
                                            ["Type"] = "Down",
                                            ["MouseHit"] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0)
                                        }
                                    }
                                    game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                                end)
                                TP(game.Workspace.AllNPC["DUN"].CFrame * MethodDun)
                            end
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoDungeon then
                if OldWorld then
                    PosDun = CFrame.new(2530.943359375, 103.69718170166016, -1931.259033203125)
                    if (PosDun.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 5 then
                        repeat wait()
                            TP(PosDun)
                        until not _G.AutoDungeon or game.PlaceId == 5931540094
                    end
                elseif NewWorld then
                    PosDunNew = CFrame.new(-4593.177734375, 223.1434326171875, -69.54349517822266)
                    if (PosDunNew.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 5 then
                        repeat wait()
                            TP(PosDunNew)
                        until not _G.AutoDungeon or game.PlaceId == 5931540094
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoDungeon then
                if DunWorld then
                    _G.AutoKillMobDun = true
                    if DunWorld then
                        for i,v in pairs(game:GetService("Players"):GetChildren()) do
                            _G.Number = i
                        end
                        if _G.ModeDun == "Normal" then
                            game:GetService("ReplicatedStorage").ChooseMapRemote:FireServer("Normal")
                        elseif _G.ModeDun == "Easy" then
                            game:GetService("ReplicatedStorage").ChooseMapRemote:FireServer("Easy")
                        elseif _G.ModeDun == "Hard" then
                            game:GetService("ReplicatedStorage").ChooseMapRemote:FireServer("Hard")
                        end
                        if game:GetService("Players").LocalPlayer.PlayerGui["GoldenArena GUI"].StartButton.Visible == true then
                            game:GetService("ReplicatedStorage").GoldenArenaEvents.StartEvent:FireServer()
                        end
                        if game:GetService("Players").LocalPlayer.PlayerGui["GoldenArena GUI"].Rise.Visible == true and _G.Number == 1 then
                            for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui["GoldenArena GUI"].Rise.LeaveButton.MouseButton1Click)) do
                                v.Function()
                            end
                        end
                    end
                end
            elseif not _G.AutoDungeon then
                _G.AutoKillMobDun = false
            end
        end)
    end
end)


