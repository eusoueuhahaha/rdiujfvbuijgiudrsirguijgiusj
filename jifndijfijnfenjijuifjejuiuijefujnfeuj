task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AutoFarmBoss then
                if game:GetService("Workspace").Monster.Boss:FindFirstChild(tostring(_G.SelectBoss)) then
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == tostring(_G.SelectBoss) then
                            repeat task.wait()
                                UseSkillFarm = true
                                getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                EquipWeapon(_G.Weapon)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                Attack()
                            until v.Humanoid.Health <= 0 or not _G.AutoFarmBoss
                            UseSkillFarm = false
                        end
                    end
                else
                    if game:GetService("ReplicatedStorage").MOB:FindFirstChild(tostring(_G.SelectBoss)) then
                        UseSkillFarm = false
                        TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild(tostring(_G.SelectBoss)):GetModelCFrame() * CFrame.new(0,0,3))
                    else
                        if _G.AutoFarmBossHop then
                            UseSkillFarm = false
                            Hop()
                        end
                    end
                end
            end
        end)
    end
end)

