task.spawn(function()
    while task.wait() do
        if _G.AutoSeaKing then
            pcall(function()
                BodyClip()
                if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                    for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do
                        if v.Name == "SeaKing" and v.Humanoid.Health > 0 then
                            repeat task.wait()
                                UseSkillFarm = true
                                EquipWeapon(_G.Weapon)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)
                                getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                Attack()
                            until v.Humanoid.Health <= 0 or not _G.AutoSeaKing
                        elseif v.Humanoid.Health <= 0 then
                            if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                                TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame * CFrame.new(0,0,2))
                            elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                                TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame * CFrame.new(0,0,2))
                            elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                                TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame * CFrame.new(0,0,2))
                            elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                                TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame * CFrame.new(0,0,2))
                            end
                        end
                    end
                elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                        TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame * CFrame.new(0,0,2))
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                        TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame * CFrame.new(0,0,2))
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                        TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame * CFrame.new(0,0,2))
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                        TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame * CFrame.new(0,0,2))
                    end
                end
                if _G.AutoSeaKingHop and not game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") and not game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") and not game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") and not game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") and not game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                    wait(3)
                    Hop()
                end
            end)
        end
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.AllBoss then
                BodyClip()
                if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
                    _G.AutoHydraMonster = true
                elseif game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                    _G.AutoSeaKing = true
                elseif game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship")then
                    _G.AutoGhostShip = true
                elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
                    _G.MsMother = true
                elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") then
                    _G.AutoEnma = true
                elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
                    _G.AutoKaido = true
                else
                    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                        if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                            TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame * CFrame.new(0,0,2))
                        elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                            TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame * CFrame.new(0,0,2))
                        elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                            TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame * CFrame.new(0,0,2))
                        elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                            TP(game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame * CFrame.new(0,0,2))
                        end
                    elseif getHydraIsland() == true then
                        pcall(function()
                            if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then 
                                if (game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Health <= 0 or not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing")) and not game:GetService("Workspace").SeaMonster:FindFirstChild("Hydra's Minion") then 
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())].HydraStand.CFrame * CFrame.new(0,2,-2)
                                    delay(12, function()
                                        pcall(function()
                                            if _G.AutoHydraMonsterHop then
                                                if (game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Health <= 0 or not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing")) and not game:GetService("Workspace").SeaMonster:FindFirstChild("Hydra's Minion") then
                                                    if game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())] ~= nil then 
                                                        game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())]:Remove()
                                                    end
                                                end
                                            end
                                        end)
                                    end)
                                end
                            else 
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island["Sea King "..tostring(getHydraIName())].HydraStand.CFrame * CFrame.new(0,2,-2)
                            end
                        end)
                    else
                        if _G.AllBossHop then
                            wait(3)
                            repeat wait()
                                Hop()
                            until a
                        end
                    end
                end
            end
        end)
    end
end)

function FindNPC(Name)
    for i,v in pairs(game:GetService("Workspace").AllNPC:GetChildren()) do
        if v.Name == Name then
            return v
        end
    end
    for i,v in pairs(game:GetService("ReplicatedStorage").MAP:GetChildren()) do
        if v.Name == Name then
            return v
        end
    end
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.LeePung then
                BodyClip()
                if game:GetService("Players").LocalPlayer.PlayerStats.Misc.Value:find("Pung Proof") then
                    if game:GetService("Players").LocalPlayer.PlayerStats.Misc.Value:find("Pung Happy") then
                        if not game:GetService("Players").LocalPlayer.PlayerStats.Misc.Value:find("Back to Lee") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Pung").CFrame
                            wait(1)
                            ClickGui_True()
                            wait(1)
                            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Pung") then
                                local ClickGui = game.Players.LocalPlayer.PlayerGui["Pung"].Dialogue.Accept
                                game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                            end
                            wait(1.5)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Lee").CFrame
                            wait(.5)
                            ClickGui_True()
                            wait(1)
                            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Lee") then
                                local ClickGui = game.Players.LocalPlayer.PlayerGui["Lee"].Dialogue.Accept
                                game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                            end
                        end
                    else
                        if game:GetService("Players").LocalPlayer.PlayerStats.Misc.Value:find("Permission Dark Beard") then
                            _G.IsBoss = "Dark Beard [Lv. 3475]"
                            if game:GetService("Workspace").Monster.Boss:FindFirstChild(_G.IsBoss) then
                                for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                                    if v.Name == _G.IsBoss and v.Humanoid.Health > 0 then
                                        repeat task.wait()
                                            UseSkillFarm = true
                                            getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                            EquipWeapon(_G.Weapon) 
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                            Attack()
                                        until _G.LeePung == false or v.Humanoid.Health <= 0
                                    end
                                end
                            else
                                if game:GetService("ReplicatedStorage").MOB:FindFirstChild(_G.IsBoss) then
                                    TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild(_G.IsBoss):GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance))
                                end
                            end
                        else
                            if game:GetService("Workspace").Monster.Boss:FindFirstChild("Sally [Lv. 3450]") then
                                for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                                    if v.Name == "Sally [Lv. 3450]" and v.Humanoid.Health > 0 then
                                        repeat task.wait()
                                            UseSkillFarm = true
                                            getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                            EquipWeapon(_G.Weapon)
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                            Attack()
                                        until _G.LeePung == false or v.Humanoid.Health <= 0
                                    end
                                end
                            else
                                if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Sally [Lv. 3450]") then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (game:GetService("ReplicatedStorage").MOB:FindFirstChild("Sally [Lv. 3450]"):GetModelCFrame() * MethodFarmLv * CFrame.new(0,0,_G.Distance))
                                end
                            end
                        end
                    end
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Lee").CFrame
                    wait(.5)
                    ClickGui_True()
                    wait(1)
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Lee") then
                        local ClickGui = game.Players.LocalPlayer.PlayerGui["Lee"].Dialogue.Accept
                        game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                    end
                    wait(1.5)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("Pung").CFrame
                    wait(1)
                    ClickGui_True()
                    wait(1)
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Pung") then
                        local ClickGui = game.Players.LocalPlayer.PlayerGui["Pung"].Dialogue.Accept
                        game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.OniMask then
                if game.Players.LocalPlayer.Character:FindFirstChild("Magma Crystal") or game.Players.LocalPlayer.Backpack:FindFirstChild("Magma Crystal") then
                    EquipWeapon("Magma Crystal")
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FindNPC("QuestLvl3375").CFrame
                    wait(1)
                    ClickGui_True()
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..tostring(3375)) then
                        local ClickGui = game.Players.LocalPlayer.PlayerGui["QuestLvl"..tostring(3375)].Dialogue.Accept
                        game:GetService("VirtualInputManager"):SendMouseButtonEvent(ClickGui.AbsolutePosition.X + 50, ClickGui.AbsolutePosition.Y + 50, 0, not game.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1), game, 0)
                    end
                else
                    if game:GetService("Workspace").Monster.Boss:FindFirstChild("The Crimson Demon [Lv. 3375]") then
                        for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                            if v.Name == "The Crimson Demon [Lv. 3375]" and v.Humanoid.Health > 0 then
                                repeat task.wait()
                                    EquipWeapon(_G.Weapon)
                                    getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                    UseSkillFarm = true
                                    Attack()
                                until v.Humanoid.Health <= 0 or not _G.OniMask or not v.Parent
                                UseSkillFarm = false
                            end
                        end
                    else
                        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("The Crimson Demon [Lv. 3375]") then
                            TP(game:GetService("ReplicatedStorage").MOB:FindFirstChild("The Crimson Demon [Lv. 3375]"):GetModelCFrame())
                        else
                            if game:GetService("Workspace").Monster.Mon:FindFirstChild("The Volcano [Lv. 3325]") then
                                for i,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                                    if v.Name == "The Volcano [Lv. 3325]" then
                                        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                            repeat task.wait()
                                                EquipWeapon(_G.Weapon)
                                                getgenv().PosMonSkill = v.HumanoidRootPart.CFrame
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * MethodFarmLv * CFrame.new(0,0,_G.Distance)
                                                UseSkillFarm = true
                                                Attack()
                                            until v.Humanoid.Health <= 0 or not _G.OniMask or not v.Parent or game.Players.LocalPlayer.Character:FindFirstChild("Magma Crystal") or game.Players.LocalPlayer.Backpack:FindFirstChild("Magma Crystal")
                                            UseSkillFarm = false
                                        end
                                    end
                                end
                            else
                                TP(FindNPC("QuestLvl3375").CFrame)
                            end
                        end
                    end
                end
            end
        end)
    end
end)

