            end
            if _G.UpDevilFruit then
                game:GetService("Players").LocalPlayer.PlayerGui.Stats.Button.StatsFrame.RemoteEvent:FireServer("Devil Fruit",Points)
            end
        end)
    end
end)


function BringPy(Name)
    LP = game:GetService('Players').LocalPlayer

    prev = LP.Character:WaitForChild('HumanoidRootPart').CFrame

    LP.Character.Parent = workspace.Terrain
local plr = game:GetService("Players").LocalPlayer
    plr.Character.Humanoid:UnequipTools()

    local Humanoid = plr.Character.Humanoid:Clone()

    local Tool = plr.Backpack:FindFirstChildOfClass("Tool")

    plr.Character.Animate.Disabled = true

    plr.Character.Humanoid:Destroy()

    Humanoid.Parent = plr.Character

    Tool.Parent = plr.Character

    Tool.Handle.Size = Vector3.new(5,5,5)

    game.Players[""..Name..""].Character.HumanoidRootPart.CFrame = CFrame.new(plr.Character[""..Tool.Name..""].Handle.Position)

    firetouchinterest(game.Players[""..Name..""].Character.HumanoidRootPart, Tool.Handle, 0);

    game:GetService('Workspace'):WaitForChild(LP.Name)

    LP.Character:WaitForChild('HumanoidRootPart').CFrame = prev
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if _G.BringPlayer then
                BringPy(_G.PlayerSelect)
            end
        end)
    end
end)

